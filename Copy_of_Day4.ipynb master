        "#DAY 3\n",
        "\n",
        "#NUMPY#NUMERICAL PYTHON#ARRAY\n",
        "#For an RGB image, the pixel values are stored as a 3-dimensional array\n",
        "import numpy as np\n",
        "a=([1,2,3],[4,5,6]) # 2 rows 3 colomn \n",
        "b=np.array(a) # convert into an array\n",
        "print(b)\n",
        "print(b.shape)# return how many rows and how many colomn\n",
        "print(b.ndim) # number of dimension (row and colomn)-2 dimension\n",
        "print(b.itemsize)# value of size is 8bit\n",
        "print(b.size) #total size of the array\n",
        "print(type(b))\n",
        "print(b.dtype) #size of the array in byte"
      ],
